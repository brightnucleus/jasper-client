{
  "name": "brightnucleus/jasper-client",
  "description": "SOAP client to interact with the Jasper Wireless Control Center.",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "license": "MIT",
  "authors": [
    {
      "name": "Alain Schlesser",
      "email": "alain.schlesser@gmail.com"
    }
  ],
  "require": {
    "brightnucleus/exceptions": "^0.3",
    "brightnucleus/config": "^0.4.3",
    "phpro/soap-client": "^0.5.2",
    "psr/log": "^1.0",
    "guzzlehttp/guzzle": "^6.2",
    "http-interop/http-factory-guzzle": "^0.1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~6",
    "zendframework/zend-code": "^3.1",
    "symfony/filesystem": "~2.8|~3.0",
    "php-vcr/php-vcr": "^1.3",
    "php-vcr/phpunit-testlistener-vcr": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "BrightNucleus\\JasperClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BrightNucleus\\JasperClient\\Tests\\": "tests/src/"
    }
  }
}
